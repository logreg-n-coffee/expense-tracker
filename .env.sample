# Next Auth URL
# NEXTAUTH_URL=

# Google Sheet information
CATEGORIES_SHEET_NAME=
EXPENSES_SHEET_NAME=
SHEET_ID=

# NEXT_PUBLIC exposes the following env to the client side
NEXT_PUBLIC_SHEET_ID=
